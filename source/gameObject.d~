import dlib.math.vector : vec3;

class GameObject
{	vec3 position;
	vec3 velocity();
	private void eachStep(float){};
	final bool step(float deltaTime)
    {
		eachStep(deltaTime);
		position += velocity * deltaTime;
		auto result = outOfArea;
		if(result){retire;}
		return result;
	}
	abstract bool outOfArea();
	void retire(){};
}
